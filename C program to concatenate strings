#include<stdio.h>
#include<conio.h>
void main()
{
    int i,j,k;
    char first_name[10]={'A','N','A','N','D','A'};
    char second_name[10]={'M','U','R','U','G','A','N'};
    char last_name[10]={'S','E','L','V','A','R','A','J'};
    char name[30];
    for(i=0;first_name[i]!='';i++)
        name[i]=first_name[i];
    name[i]=' ';
    for(j=0;second_name[j]!='';j++)
        name[i+j+1]=second_name[j];
    name[i+j+1]=' ';
    for(k=0;last_name[k]!='';k++)
        name[i+j+k+2] = '';
    printf("nn");
    printf("%sn",name);
}
